<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gmlee.test.mybatis.dao.mapper.CityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.gmlee.test.mybatis.dao.entity.City">
        <id column="id" property="id" />
        <result column="name_en" property="nameEn" />
        <result column="name_cn" property="nameCn" />
        <result column="province_name_cn" property="provinceNameCn" />
        <result column="province_name_en" property="provinceNameEn" />
        <result column="latitude" property="latitude" />
        <result column="longitude" property="longitude" />
        <result column="enable_order" property="enableOrder" />
        <result column="enable_eorder" property="enableEorder" />
        <result column="revision" property="revision" />
        <result column="enable_vip" property="enableVip" />
        <result column="enable_sticker" property="enableSticker" />
        <result column="enable_register" property="enableRegister" />
        <result column="enable_deposit" property="enableDeposit" />
        <result column="protocol_revision" property="protocolRevision" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name_en, name_cn, province_name_cn, province_name_en, latitude, longitude, enable_order, enable_eorder, revision, enable_vip, enable_sticker, enable_register, enable_deposit, protocol_revision
    </sql>

</mapper>
